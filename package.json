{
  "name": "vsc-apm-pulse",
  "displayName": "APM Pulse",
  "description": "Real-time, game-style APM counter for your VS Code status bar",
  "icon": "docs/mouse.png",
  "version": "0.1.0",
  "author": "jseashell",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jseashell/vsc-apm-pulse.git"
  },
  "license": "MIT",
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.92.0",
    "node": ">=20.16.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Other",
    "Visualization"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/node": "20.x",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
